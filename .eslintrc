{
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "es6": true
    },
    "parser": "babel-eslint",
    "rules": {
        "quotes": [2, "single"],
        "indent": [ 1, "tab" ],
        "react/jsx-indent": [ 1, "tab" ],
        "no-tabs": 0
    },
    "plugins": [
        "react"
    ],
    "extends": "airbnb"
}
